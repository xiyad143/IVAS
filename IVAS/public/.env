# Netlify Configuration
SITE_NAME=ivas-sms-analyzer
URL=https://ivas-sms-analyzer.netlify.app

# Security
SESSION_TIMEOUT=3600

# CORS
ALLOWED_ORIGINS=https://ivas-sms-analyzer.netlify.app,http://localhost:8888

# iVAS Configuration
IVAS_BASE_URL=https://www.ivasms.com
IVAS_SMS_PAGE=/portal/live/test_sms
IVAS_TIMEOUT=30000

# Rate Limiting (per IP)
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Cache Settings
CACHE_TTL=30
CACHE_MAX_AGE=300

# Debug
NODE_ENV=production
LOG_LEVEL=info
